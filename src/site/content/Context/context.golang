type favContextKey string

f := func(ctx context.Context, k favContextKey) {
	if v := ctx.Value(k); v != nil {
		fmt.Printf("Valor Encontrado para a key %v: %v\n", k, v)
		return
	}
	fmt.Printf("key n√£o encontrada: %v\n", k)
}

k := favContextKey("linguagem")
